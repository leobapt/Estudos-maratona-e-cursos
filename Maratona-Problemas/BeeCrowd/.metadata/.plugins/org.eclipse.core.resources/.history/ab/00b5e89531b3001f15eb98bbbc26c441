package exercicios;
import java.util.Scanner;

public class Exercicio1021 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		
		double valorTotal = sc.nextDouble();
		
		final int[] valores_reais = {100,50,20,10,2};
		final double[] valores_moedas = {1.0, 0.5, 0.25, 0.10, 0.05, 0.01};
		
		for(int i = 0; i < valores_reais.length; i++) {
			int quantidadeNotas = valorTotal / valores_reais;
		}
		
		
		sc.close();
	}

}
