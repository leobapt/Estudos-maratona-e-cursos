package exercicios;
import java.util.Scanner;
public class Exercicio18 {

	public static void main(String[] args) {
		// Cria uma inst√¢ncia para capturar dados
		Scanner sc = new Scanner(System.in);
		
		 //atributos com valores das notas em reais
		 final int nota100 = 100;
		 final int nota50 = 50;
		 final int nota20 = 20;
		 final int nota10 = 10;
		 final int nota5 = 5;
		 final int nota2 = 2;
		 final int nota1 = 1;
		 
		 int quantidadeTotalReais = sc.nextInt();
		 
		 int quantidadeTotal100Reais = quantidadeTotalReais / nota100;
		 quantidadeTotalReais %= nota100;
		 
		 int quantidadeTotal50Reais = quantidadeTotalReais / nota50;
		 quantidadeTotalReais %= nota50;
		 
		 int quantidadeTotal20Reais = quantidadeTotalReais / nota20;
		 quantidadeTotalReais %= nota20;
		 
		 int quantidadeTotal10Reais = quantidadeTotalReais / nota10;
		 quantidadeTotalReais %= nota10;
		 
		 int quantidadeTotal5Reais = quantidadeTotalReais / nota5;
		 quantidadeTotalReais %= nota5;
		 
		 int quantidadeTotal2Reais = quantidadeTotalReais / nota2;
		 quantidadeTotalReais %= nota2;
		 
		 int quantidadeTotal1Real = quantidadeTotalReais / nota1;
		 quantidadeTotalReais %= nota1;
		 
		 System.out.println(quantidadeTotal100Reais + " nota(s) de R$ 100,00");
		 System.out.println(quantidadeTotal50Reais + " nota(s) de R$ 50,00");
		 System.out.println(quantidadeTotal20Reais + " nota(s) de R$ 20,00");
		 System.out.println(quantidadeTotal10Reais + " nota(s) de R$ 10,00");
		 System.out.println(quantidadeTotal5Reais + " nota(s) de R$ 5,00");
		 System.out.println(quantidadeTotal2Reais + " nota(s) de R$ 2,00");
		 System.out.println(quantidadeTotal100Reais + " nota(s) de R$ 100,00");
		sc.close();
	}

}
