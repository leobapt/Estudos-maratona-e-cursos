package exercicios;
import java.util.Scanner;
public class Exercicio18 {

	public static void main(String[] args) {
		// Cria uma inst√¢ncia para capturar dados
		Scanner sc = new Scanner(System.in);
		
		 //atributos com valores das notas em reais
		 final int nota100 = 100;
		 final int nota50 = 50;
		 final int nota20 = 20;
		 final int nota10 = 10;
		 final int nota5 = 5;
		 final int nota2 = 2;
		 final int nota1 = 1;
		 
		 int quantidadeNotas100 = 0;
		 
		 int quantidadeTotal_Reais = sc.nextInt();
		 
		 if(quantidadeTotal_Reais % nota100 == 0 ) {
			quantidadeNotas100++;
		 }
		 
		 System.out.println(quantidadeNotas100 + "Nota");
		 
	
		sc.close();
	}

}
